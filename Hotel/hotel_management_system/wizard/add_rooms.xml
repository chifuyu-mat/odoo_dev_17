<odoo>
    <data>
        <record id="view_hotel_add_rooms_wizard_form" model="ir.ui.view">
            <field name="name">hotel.add.rooms.wizard.form</field>
            <field name="model">hotel.add.rooms.wizard</field>
            <field name="arch" type="xml">
                <form string="Add Rooms">
                    <sheet>
                        <group>
                            <field name="room_ids" widget="many2many_tags" />
                        </group>
                        <footer>
                            <button string="Add Rooms" type="object" name="action_add_rooms"
                                class="btn-primary" />
                            <button string="Cancel" class="btn-secondary" special="cancel" />
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>